{
  "embedding": {
    "provider": "VoyageAI",
    "apiUrl": "https://api.voyageai.com/v1/",
    "apiKey": "",
    "model": "voyage-code-3",
    "dimension": 2048,
    "precision": "Float32",
    "batchSize": 50,
    "maxRetries": 6,
    "retryDelayMs": 1000
  },
  "parser": {
    "includeFilePath": false,
    "includeProjectContext": false,
    "maxChunkSize": 2000,
    "enableSlidingWindow": true
  },
  "performance": {
    "enableAvx512": "Auto",
    "memoryLimit": 1024,
    "maxDegreeOfParallelism": 16
  },
  "api": {
    "enableRestApi": false,
    "restApiPort": 8080,
    "enableMcp": true
  },
  "monitoring": {
    "enableAutoReindex": true,
    "verifyOnStartup": true,
    "debounceDelaySeconds": 60,
    "enableFileWatching": true
  }
}