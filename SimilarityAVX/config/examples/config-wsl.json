{
  "embedding": {
    "provider": "VoyageAI",
    "apiUrl": "https://api.voyageai.com/v1/",
    "apiKey": "",
    "model": "voyage-code-3",
    "dimension": 2048,
    "precision": "Float32",
    "batchSize": 50,
    "maxRetries": 6,
    "retryDelayMs": 1000
  },
  "parser": {
    "maxChunkSize": 2000
  },
  "monitoring": {
    "enableAutoReindex": true,
    "verifyOnStartup": true,
    "debounceDelaySeconds": 60,
    "enableFileWatching": false,
    "enablePeriodicRescan": true,
    "periodicRescanMinutes": 30
  },
  "security": {
    "allowedDirectories": ["/mnt/e/", "/home/"],
    "enablePathValidation": true
  },
  "api": {
    "enableMcp": true
  }
}